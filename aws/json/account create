aws budgets create-budget \
    --account-id  622563537239\
    --budget file://aws/json/budget.json \
    --notifications-with-subscribers file://aws/json/notifications-with-subscribers.json


aws sns subscribe \
    --topic-arn="arn:aws:sns:eu-west-2:622563537239:billing-alarm" \
    --protocol=email \
    --notification-endpoint=mbonu.mike@yahoo.com